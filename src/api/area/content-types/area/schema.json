{
  "kind": "collectionType",
  "collectionName": "areas",
  "info": {
    "singularName": "area",
    "pluralName": "areas",
    "displayName": "Area",
    "description": "Create your blog content"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "localization": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
